               ],
               "Type": "SpringEnum",
               "MinValue": null,
               "MaxValue": null,
               "DefaultValue": "core_dummy"
            },
            {
               "Name": "IncludeDir4",
               "DisplayName": "Unleashed Physics",
               "Description": [
                  "Enable Sonic Unleashed Physics, which enable a longer homing attack, airboost and dash (Not recomended for Sonic Generations stages as those stages were not designed around these physics",
               ],
               "Type": "PhysicsEnum",
               "MinValue": null,
               "MaxValue": null,
               "DefaultValue": "core_dummy"
            },

            {
               "Name": "IncludeDir5",
               "DisplayName": "Debug Boot Mode",
               "Description": [
                  "Enable Debug Boot Mode, this fixes misssing sound effects such as the quickstep sound (Not recomended when playing the game with stage mods)",
               ],
               "Type": "DebugEnum",
               "MinValue": null,
               "MaxValue": null,
               "DefaultValue": "core_debug"
            },

            
         ]
      }
